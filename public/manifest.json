{
  "manifest_version": 3,
  "name": "Swift Trader",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}